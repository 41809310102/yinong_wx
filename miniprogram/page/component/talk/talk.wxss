
.top_box {
  width: 100%;
  background: #fbbd08;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.top_list {
  width: 50rpx;
  height: 50rpx;
}

.search_box {
  width: 620rpx;
  height: 66rpx;
  border-radius: 33rpx;
  background: #fff;
  display: flex;
  align-items: center;
}

.search_ico {
  padding-left: 30rpx;
  width: 32rpx;
  height: 32rpx;
}

.search_text {
  padding-left: 10rpx;
  font-size: 28rpx;
  letter-spacing: 2rpx;
  color: rgb(167, 167, 167);
}
.top_contain {
      width: 100%;
      display: flex;
      box-sizing: border-box;
      padding: 20rpx 36rpx 30rpx 0;
      background:linear-gradient(to right, red , blue);
      position: relative;
      justify-content: flex-end;
     
}

.des_contain {
      width: 618rpx;
      height: 157rpx;
      border-radius: 10rpx;
      background: #fff;
      display: flex;
      box-sizing: border-box;
      flex-direction: column;
      justify-content: center;
      padding: 0 0 0 90rpx;
}

.name {
      font-size: 36rpx;
      line-height: 50rpx;
}

.sign {
      margin-top: 10rpx;
      font-size: 28rpx;
      letter-spacing: 3rpx;
      line-height: 45rpx;
      color: #b8b8b8;
}

.avator_box {
      position: absolute;
      left: 0rpx;
      top: 0rpx;
      display: flex;
      padding: 44rpx 0 0 40rpx;
}

.avator {
      box-shadow: 0 0 10rpx #b8b8b8;
      width: 110rpx;
      height: 110rpx;
      border-radius: 20rpx;
}

@import '../../common/common.wxss';

page, .main {
  height: 100%;
}

.meun-city-txt {
  display: block;
  border-bottom: 1rpx solid #e9e9e9;
